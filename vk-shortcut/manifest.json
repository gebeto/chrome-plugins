{
  "name": "VK Shortcuts",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Shortcuts in VK",
  "homepage_url": "https://github.com/gebeto/chrome-plugins",
  "minimum_chrome_version": "21",
  "content_scripts": [
    { "matches": ["https://vk.com/*"], "js": ["main.js"], "run_at": "document_end" }
  ],
  "permissions":["https://vk.com/*"]
}
