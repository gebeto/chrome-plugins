{
  "name": "NULP Timer",
  "version": "0.3",
  "manifest_version": 2,
  "description": "Tweak for NULP website",
  "homepage_url": "https://github.com/gebeto/chrome-plugins",
  "minimum_chrome_version": "21",
  "content_scripts": [
    { "matches": ["http://lp.edu.ua/*", "http://lpnu.ua/*"], "js": ["main_end.js"], "run_at": "document_end" }
  ],
  "permissions":["http://lp.edu.ua/*", "http://lpnu.ua/*"]
}
